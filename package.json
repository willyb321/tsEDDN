{
	"name": "ts-eddn",
	"version": "0.1.0",
	"description": "ts-eddn",
	"license": "MIT",
	"repository": "willyb321/tsEDDN",
	"author": "Willyb321 (https://williamblythe.info)",
	"keywords": [
		"Elite: Dangerous",
		"E:D",
		"EDDN",
		"Elite Dangerous Data Network"
	],
	"bugs": {
		"url": "https://github.com/willyb321/tsEDDN/issues"
	},
	"files": [
		"lib"
	],
	"main": "lib/index.js",
	"typings": "lib/index.d.ts",
	"scripts": {
		"clean": "rimraf lib && rimraf coverage && rimraf .nyc_output && rimraf lib_test",
		"dev": "concurrently \"npm run watch\" \"nodemon lib/index.js\"",
		"lint": "tslint --force --format verbose \"src/**/*.ts\"",
		"prepublish": "npm run build",
		"prebuild": "npm run clean && npm run lint && echo Using TypeScript && tsc --version",
		"build": "tsc --pretty",
		"test": "npm run clean && tsc -p tsconfig.test.json --pretty && nyc --exclude \"**/*-spec.js\" ava \"**/*-spec.js\" --verbose",
		"coverage": "nyc report --reporter=lcov --reporter=text --reporter=html",
		"watch": "npm run build -- --watch",
		"watch:test": "npm run test -- --watch"
	},
	"dependencies": {
		"@types/express": "^4.0.36",
		"@types/lodash": "^4.14.71",
		"@types/mongodb": "^2.2.7",
		"@types/raven": "^1.2.4",
		"@types/zeromq": "^4.5.0",
		"dotenv": "^4.0.0",
		"express": "^4.15.3",
		"express-rate-limit": "^2.8.1",
		"is-dev": "^0.1.4",
		"lodash": "^4.17.4",
		"moment": "^2.18.1",
		"mongodb": "^2.2.30",
		"raven": "^2.1.0",
		"zeromq": "^4.5.0"
	},
	"devDependencies": {
		"@types/node": "^8.0.16",
		"ava": "^0.21.0",
		"concurrently": "^3.5.0",
		"coveralls": "^2.13.1",
		"nodemon": "^1.11.0",
		"nyc": "^11.0.3",
		"rimraf": "^2.6.1",
		"tslint": "^5.5.0",
		"tslint-config-xo": "^1.3.0",
		"typescript": "^2.4.2"
	},
	"engines": {
		"node": "^8"
	},
	"ava": {
		"require": [
			"babel-register"
		]
	}
}
