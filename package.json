{
	"name": "ts-eddn",
	"version": "0.1.0",
	"description": "ts-eddn",
	"license": "MIT",
	"repository": "willyb321/tsEDDN",
	"author": "Willyb321 (https://williamblythe.info)",
	"keywords": [
		"Elite: Dangerous",
		"E:D",
		"EDDN",
		"Elite Dangerous Data Network"
	],
	"bugs": {
		"url": "https://github.com/willyb321/tsEDDN/issues"
	},
	"files": [
		"lib"
	],
	"main": "lib/index.js",
	"typings": "lib/index.d.ts",
	"scripts": {
		"clean": "rimraf lib && rimraf coverage && rimraf .nyc_output && rimraf lib_test",
		"dev": "concurrently \"npm run watch\" \"nodemon lib/index.js\"",
		"lint": "tslint --force --format verbose \"src/**/*.ts\"",
		"build": "tsc --pretty",
		"test": "npm run clean && tsc -p tsconfig.test.json --pretty && nyc --exclude \"**/*-spec.js\" ava \"**/*-spec.js\" --verbose",
		"coverage": "nyc report --reporter=lcov --reporter=text --reporter=html",
		"watch": "npm run build -- --watch",
		"watch:test": "npm run test -- --watch"
	},
	"dependencies": {
		"dotenv": "^4.0.0",
		"express": "^4.16.2",
		"express-rate-limit": "^2.9.0",
		"generate-schema": "^2.5.1",
		"is-dev": "^0.1.4",
		"lodash": "^4.17.4",
		"moment": "^2.19.1",
		"mongodb": "^2.2.33",
		"mongoose": "^4.12.4",
		"mongoose-gen": "^2.1.1",
		"mongoose-paginate": "^5.0.3",
		"raven": "^2.2.1",
		"zeromq": "^4.6.0"
	},
	"devDependencies": {
		"@types/dotenv": "^4.0.1",
		"@types/express": "^4.0.37",
		"@types/express-rate-limit": "^2.8.0",
		"@types/lodash": "^4.14.78",
		"@types/mongodb": "^2.2.14",
		"@types/mongoose": "^4.7.24",
		"@types/mongoose-paginate": "^5.0.6",
		"@types/node": "^8.0.46",
		"@types/raven": "^2.1.2",
		"@types/rimraf": "^2.0.2",
		"@types/zeromq": "^4.5.1",
		"ava": "^0.22.0",
		"concurrently": "^3.5.0",
		"coveralls": "^3.0.0",
		"nodemon": "^1.12.1",
		"nyc": "^11.3.0",
		"rimraf": "^2.6.2",
		"tslint": "^5.8.0",
		"tslint-config-xo": "^1.3.0",
		"typescript": "^2.7.0-dev.20171021"
	},
	"engines": {
		"node": "^8"
	},
	"ava": {
		"require": [
			"babel-register"
		]
	}
}
